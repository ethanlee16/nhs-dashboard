{
  "scripts": {
    "start": "node ./bin/www",
    "dev": "concurrently -k \"nodemon ./bin/www\" \"npm run watch\"",
    "watch": "watchify client/App.js -o public/bundle.js -t [ babelify --presets [ es2015 stage-0 react ] ]",
    "cron": "node bin/cron",
    "postinstall": "browserify client/App.js -o public/bundle.js -t [ babelify --presets [ es2015 stage-0 react ] ]"
  },
  "engines": {
    "node": ">=6.3.0"
  },
  "dependencies": {
    "babel-eslint": "^6.1.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "body-parser": "~1.13.2",
    "browserify": "^13.0.1",
    "express": "~4.13.1",
    "firebase": "^3.4.0",
    "fullcalendar": "^3.0.0",
    "hbs": "^4.0.0",
    "jquery": "^3.1.0",
    "lodash": "^4.15.0",
    "morgan": "~1.6.1",
    "phone": "^1.0.8",
    "promise-polyfill": "^6.0.2",
    "react": "^15.2.1",
    "react-bootstrap": "^0.30.3",
    "react-dom": "^15.2.1",
    "react-router": "^2.8.1",
    "request-promise": "^4.1.1",
    "twilio": "^2.10.0",
    "whatwg-fetch": "^2.0.0"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "eslint": "^3.0.1",
    "eslint-plugin-react": "^5.2.2",
    "nodemon": "^1.9.2",
    "watchify": "^3.7.0"
  }
